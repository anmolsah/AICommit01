{
  "name": "commit-genius",
  "displayName": "Commit Genius - AI Git Commit Message Generator",
  "description": "ðŸš€ AI-powered Git commit message generator for VS Code. Auto-generate professional, conventional commit messages using OpenAI, Claude, Gemini & more. Free & premium models available.",
  "version": "3.2.7",
  "publisher": "aicommit-publisher",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "SCM Providers",
    "Machine Learning",
    "Other"
  ],
  "keywords": [
    "git",
    "commit",
    "ai",
    "artificial intelligence",
    "commit message",
    "conventional commits",
    "openai",
    "claude",
    "gemini",
    "source control",
    "scm",
    "automation",
    "productivity",
    "developer tools",
    "git workflow",
    "commit generator",
    "ai assistant",
    "code quality",
    "version control",
    "git integration",
    "commit standards",
    "conventional",
    "semantic commits",
    "auto commit",
    "smart commits",
    "gpt",
    "llm",
    "machine learning",
    "deepseek",
    "mistral",
    "qwen",
    "openrouter"
  ],
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anmolsah/AICommit01"
  },
  "homepage": "https://github.com/anmolsah/AICommit01",
  "bugs": {
    "url": "https://github.com/anmolsah/AICommit01/issues"
  },
  "qna": "marketplace",
  "sponsor": {
    "url": "https://github.com/sponsors/anmolsah"
  },
  "activationEvents": [
    "workspaceContains:.git"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "commit-genius.generateAICommitMessage",
        "title": "Commit Genius: Generate AI Commit Message",
        "category": "Commit Genius",
        "icon": "$(sparkle)"
      },
      {
        "command": "commit-genius.setApiKey",
        "title": "Commit Genius: Set OpenRouter API Key",
        "category": "Commit Genius"
      }
    ],
    "menus": {
      "scm/title": [
        {
          "when": "scmProvider == 'git'",
          "command": "commit-genius.generateAICommitMessage",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "Commit Genius",
      "properties": {
        "commit-genius.model": {
          "type": "string",
          "default": "moonshotai/kimi-k2:free",
          "description": "The AI model to use for generating commit messages from OpenRouter.",
          "enum": [
            "deepseek/deepseek-chat-v3-0324:free",
            "qwen/qwen3-coder:free",
            "mistralai/mistral-small-3.2-24b-instruct:free",
            "openai/gpt-oss-120b:free",
            "moonshotai/kimi-k2:free",
            "meta-llama/llama-3.3-70b-instruct:free",
            "nvidia/llama-3.1-nemotron-ultra-253b-v1:free",
            "deepseek/deepseek-r1-0528:free",
            "openai/gpt-oss-120b",
            "anthropic/claude-sonnet-4",
            "google/gemini-2.5-pro",
            "x-ai/grok-code-fast-1",
            "deepseek/deepseek-chat-v3.1",
            "qwen/qwen3-30b-a3b-instruct-2507",
            "openai/gpt-5"
          ],
          "enumDescriptions": [
            "Deepseek 71B (Free)",
            "Qwen 7B (Free)",
            "Mistral 7B Instruct (Free)",
            "OpenAI GPT-OSS 120B (Free)",
            "Moonshot Kimi K2 (Free)",
            "Meta Llama 3.3 70B Instruct (Free)",
            "NVIDIA Llama 3.1 253B (Free)", 
            "Deepseek 7B (Free)",
            "OpenAI GPT-OSS 120B (Paid)",
            "Anthropic Claude Sonnet 4 (Paid)",
            "Google Gemini 2.5 Pro (Paid)",
            "X-AI Grok Code Fast 1 (Paid)",
            "Deepseek 71B (Paid)",
            "Qwen 30B (Paid)",
            "OpenAI GPT-5 (Paid)"
          ]
        }
      }
    }
  },
  "scripts": {
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
